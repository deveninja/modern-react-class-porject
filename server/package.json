{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for my Stream app",
  "main": "server.js",
  "scripts": {
    "start": "json-server -p 3001 -w ./database/db.json",
    "server": "npm-run-all --parallel server:*",
    "server:dev": "nodemon server.js",
    "server:app": "cd.. && cd client && npm run start",
    "server:json": "json-server -p 3001 -w ./database/db.json"
  },
  "author": "DeveNinja",
  "license": "MIT",
  "dependencies": {
    "json-server": "^0.14.2",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5"
  }
}
